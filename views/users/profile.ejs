<div class="page-header">
    <h1><%= user.name %></h1>    
</div>
<hr>
<div id="newEtchBtn">
    <h3><a href="/etches/new">New Etch</a></h3>
</div>
<div id="etchList">
    <h4>Previous etches</h4>
    <ul>
        <% etches.forEach(function(e) { %>
            <div class="profile-box"><li><a href="/etches/<%= e._id %>"><%= e.title %></a></li></div>
        <% }) %>
    </ul>
</div
<div class="row">
    <div class="col-sm-6 col-sm-offset-3">
        <div class="well">
            <h3>Account info</h3>
                <p>
                    <strong>Name</strong>: <%= user.name %><br>
                    <strong>email</strong>: <%= user.email %><br>
                </p>
        </div>
    </div>
</div>